    <script>

        function MisafirEmailTelefonKontrol() {


            Notiflix.Block.Standard('.login-box');
            $.ajax({
                type: "post",
                url:"/Login/LoginCheck",
                data: JSON.stringify({
                    "FirmaId": $("#FirmaId").val(),
                    "BirthDate": $("#day").val() + "." + $("#month").val() + "." + $("#year").val(),
                    "Email": $("#guestemail").val(),
                    "PhoneNumber": $("#guestPhoneNumber").val(),
                    "RoomNumber": $("#roomNumber").val(),
                    "HotspotType" : "Guest"
                }),
                success: function (result) {

                    if (result.success) {

                        return true;
 
                    } else {
                        Notiflix.Notify.Failure("Bilgiler Eşleşmedi");
                        return false;
                    }
                    Notiflix.Block.Remove("*", 0);
                }

            });


        }

        function ToplantiEmailTelefonKontrol() {


            Notiflix.Block.Standard('.login-box');
            $.ajax({
                type: "post",
                url: "/Login/LoginCheck",
                data: JSON.stringify({
                    "FirmaId": $("#FirmaId").val(),
                    "BirthDate": $("#meetIdNumber").val(),
                    "Email": $("#meetEmail").val(),
                    "PhoneNumber": $("#meetPhoneNumber").val(),
                    "RoomNumber": $("#meetPassword").val(),
                    "HotspotType": "Meet"
                }),
                success: function (result) {

                    if (result.success) {

                        return true;

                    } else {
                        Notiflix.Notify.Failure("Bilgiler Eşleşmedi");
                        return false;
                    }
                    Notiflix.Block.Remove("*", 0);
                }

            });
        }

        function SpaEmailTelefonKontrol() {


            Notiflix.Block.Standard('.login-box');
            $.ajax({
                type: "post",
                url: "/Login/LoginCheck",
                data: JSON.stringify({
                    "FirmaId": $("#FirmaId").val(),
                    "BirthDate": $("#meetIdNumber").val(),
                    "Email": $("#meetEmail").val(),
                    "PhoneNumber": $("#meetPhoneNumber").val(),
                    "RoomNumber": $("#meetPassword").val(),
                    "HotspotType": "Spa"
                }),
                success: function (result) {

                    if (result.success) {

                        return true;

                    } else {
                        Notiflix.Notify.Failure("Bilgiler Eşleşmedi");
                        return false;
                    }
                    Notiflix.Block.Remove("*", 0);
                }

            });


        }

        function MikrotikUcretli() {
            document.sendin.username.value = document.ucretli.pricePassword.value;
            document.sendin.password.value = hexMD5('$(chap-id)' + "1071" + '$(chap-challenge)');
            document.sendin.submit();
            return false;
        }

        function MikrotikUcretsiz() {

            if ($("#MisafirEmailZorunlu").val() == "True") {
                if ($("#guestemail").val() == null) {
                    Notiflix.Notify.Failure("Lütfen Email Adresi Giriniz");
                    return false;
                }
            }

            if ($("#MisafirTelefonZorunlu").val() == "True") {
                if ($("#guestPhoneNumber").val() == null) {
                    Notiflix.Notify.Failure("Lütfen Telefon Numaranızı Giriniz");
                    return false;
                }
            }


            if ($("#MisafirEmail").val() == "True") {
                var snc = MisafirEmailTelefonKontrol();
            }
     
            document.sendin.username.value = document.ucretli.day.value + "." + document.ucretli.month.value + "." + document.ucretli.year.value;
            document.sendin.password.value = hexMD5('$(chap-id)' + document.ucretli.roomNumber.value + '$(chap-challenge)');
            document.sendin.submit();
            return false;
        }

        function MikrotikToplanti() {

            if ($("#ToplantiEmailZorunlu").val() == "True") {
                if ($("#meetEmail").val() == null) {
                    Notiflix.Notify.Failure("Lütfen Email Adresi Giriniz");
                    return false;
                }
            }

            if ($("#ToplantiTelefonZorunlu").val() == "True") {
                if ($("#meetPhoneNumber").val() == null) {
                    Notiflix.Notify.Failure("Lütfen Telefon Numaranızı Giriniz");
                    return false;
                }
            }

            if ($("#ToplantiEmail").val() == "True") {
                var snc = ToplantiEmailTelefonKontrol();
            }
            document.sendin.username.value = document.toplanti.meetIdNumber.value;
            document.sendin.password.value = hexMD5('$(chap-id)' + document.toplanti.meetPassword.value + '$(chap-challenge)');
            document.sendin.submit();
            return false;
        }

        function MikrotikSpa() {

            if ($("#SpaEmailZorunlu").val() == "True") {
                if ($("#spaEmail").val() == null) {
                    Notiflix.Notify.Failure("Lütfen Email Adresi Giriniz");
                    return false;
                }
            }

            if ($("#SpaTelefonZorunlu").val() == "True") {
                if ($("#spaPhoneNumber").val() == null) {
                    Notiflix.Notify.Failure("Lütfen Telefon Numaranızı Giriniz");
                    return false;
                }
            }

            if ($("#SpaEmail").val() == "True") {
                var snc = ToplantiEmailTelefonKontrol();
            }
            document.sendin.username.value = document.toplanti.meetIdNumber.value;
            document.sendin.password.value = hexMD5('$(chap-id)' + document.toplanti.meetPassword.value + '$(chap-challenge)');
            document.sendin.submit();
            return false;
        }

        function MikrotikPersonel() {
            document.sendin.username.value = document.personel.staffIdNumber.value;
            document.sendin.password.value = hexMD5('$(chap-id)' + document.personel.staffPassword.value + '$(chap-challenge)');
            document.sendin.submit();
            return false;
        }
