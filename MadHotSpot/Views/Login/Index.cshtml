                            <li class="nav-item">
                                <a class="nav-link active" id="tabs_guest_tab" data-toggle="pill" href="#tabs_guest" role="tab" aria-controls="tabs_guest" aria-selected="true">
                                    @if (Model.UcretsizHotspot)
                                    {
                                        <span>Misafir</span>
                                    }
                                    else
                                    {
                                        <span>İnternet</span>
                                    }
                                </a>
                            </li>
                            @if (Model.ToplantiGirisi)
                            {
                                <li class="nav-item">
                                    <a class="nav-link" id="tabs_meet_tab" data-toggle="pill" href="#tabs_meet" role="tab" aria-controls="tabs_meet" aria-selected="false">Toplantı</a>
                                </li>
                            }
                            @if (Model.PersonelGirisi)
