    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Hotspot Login</title>
    <!-- Font Awesome -->
    <link rel="stylesheet" href="~/plugins/AdminLTE/plugins/fontawesome-free/css/all.min.css">
    <!-- icheck bootstrap -->
    <link rel="stylesheet" href="~/plugins/AdminLTE/plugins/icheck-bootstrap.min.css">
    <!-- Theme style -->
    <link href="~/plugins/AdminLTE/css/adminlte.min.css" rel="stylesheet" />
    <link href="~/plugins/particles/css/particles.css" rel="stylesheet" />

    <style>
        label:not(.form-check-label):not(.custom-file-label) {
            font-weight: 400 !important;
        }

        label {
            display: contents !important;
            margin-bottom: 0.5rem;
        }

        .card-body {
            padding-left: 0.25rem !important;
            padding-right: 0.25rem !important;
        }
    </style>
