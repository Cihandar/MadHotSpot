        <div class="card card-outline card-primary">
            <div class="card-header text-center">
                @if (Model.LogoUrl != null)
                {
                    <img width="200" src="/images/OtelLogo/@Model.LogoUrl" />
                }
                else
                {
                    <b>Hotspot</b> <p>Login</p>
                }
