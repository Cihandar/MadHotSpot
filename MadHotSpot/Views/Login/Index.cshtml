@{
    Layout = null;
    @model MadHotSpot.Models.HotSpotAyar;

    string gemail = "false";
    string gemailz = "false";
    string gtel = "false";
    string gtelz = "false";

    string temail = "false";
    string temailz = "false";
    string ttel = "false";
    string ttelz = "false";

    string semail = "false";
    string semailz = "false";
    string stel = "false";
    string stelz = "false";

    string gbemail = "false";
    string gbemailz = "false";
    string gbtel = "false";
    string gbtelz = "false";


    if (Model.MisafirEmail) gemail = "true";
    if (Model.MisafirEmailZorunlu) gemailz = "true";
    if (Model.MisafirTelefon) gtel = "true";
    if (Model.MisafirTelefonZorunlu) gtelz = "true";

    if (Model.ToplantiEmail) temail = "true";
    if (Model.ToplantiEmailZorunlu) temailz = "true";
    if (Model.ToplantiTelefon) ttel = "true";
    if (Model.ToplantiTelefonZorunlu) ttelz = "true";

    if (Model.SpaEmail) semail = "true";
    if (Model.SpaEmailZorunlu) semailz = "true";
    if (Model.SpaTelefon) stel = "true";
    if (Model.SpaTelefonZorunlu) stelz = "true";

    if (Model.GunuBirlikEmail) gbemail = "true";
    if (Model.GunuBirlikEmailZorunlu) gbemailz = "true";
    if (Model.GunuBirlikTelefon) gbtel = "true";
    if (Model.GunuBirlikTelefonZorunlu) gbtelz = "true";

}

