                                    <a class="nav-link" id="tabs_staff_tab" data-toggle="pill" href="#tabs_staff" role="tab" aria-controls="tabs_staff" aria-selected="false">Personel</a>
                                </li>
                            }
                            @if (Model.SpaGirisi)
